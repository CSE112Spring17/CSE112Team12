{
  "name": "cse112project",
  "version": "1.0.0",
  "description": "Node application with Mocha/Enzyme",
  "main": "index.js",
  "engine": "node >= 4.2.1",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "nyc --reporter=html mocha --use_strict --harmony",
    "start": "node --use_strict index.js",
    "dev": "nodemon --use_strict index.js",
    "bundle": "webpack",
    "bundle-prod": "webpack -p",
    "bundle-dev": "webpack -d --watch",
    "jshint": "jshint -c .jshintrc .",
    "esdoc": "esdoc",
    "config": "node nightwatch.conf.BASIC.js",
    "e2e": "node_modules/nightwatch/bin/nightwatch --config nightwatch.conf.BASIC.js"
  },
  "pre-commit": [
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safarsi/CSE112Team12.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/safarsi/CSE112Team12/issues"
  },
  "homepage": "https://github.com/safarsi/CSE112Team12#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "enzyme": "^2.8.2",
    "esdoc": "^0.5.2",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.15",
    "nodemon": "^1.11.0",
    "selenium-download": "^2.0.10",
    "nyc": "^10.3.2",
    "pre-commit": "^1.2.2",
    "should": "^11.2.1",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "material-ui": "^0.16.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "validator": "^6.2.0"
  }
}
